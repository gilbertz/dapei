<% unless @properties.blank? %>
<table class="table">
    <tr>
      <th>ID</th>
      <th>Name</th>
      <th>Created at</th>
    </tr>

    <% @properties.each do |p| %>
    <tr>
      <td><%= p.id %></td>
      <td><%= p.name %></td>
      <td><%= p.created_at %></td>
    </tr>
    <% end %>
</table>
<% end %>