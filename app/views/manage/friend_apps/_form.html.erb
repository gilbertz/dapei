<%= form_for [:manage, @friend_app], html: {role: "form"} do |f| %>

    <div class="form-group">
      <%= f.label :order_no %>
      <%= f.text_field :order_no, class: "form-control" %>
    </div>

    <div class="form-group">
      <%= f.label :app_name %>
      <%= f.text_field :app_name, class: "form-control" %>
    </div>

    <div class="form-group">
      <%= f.label :description %>
      <%= f.text_field :description, class: "form-control" %>
    </div>

    <div class="form-group">
      <%= f.label :icon_url %>
      <%= f.text_field :icon_url, class: "form-control" %>
    </div>

    <div class="form-group">
      <%= f.label :download_url %>
      <%= f.text_field :download_url, class: "form-control" %>
    </div>

    <div class="form-group">
      <label for=""></label>
      <%= f.submit %>
    </div>

<% end %>