<xml>
    <ToUserName><![CDATA[<%= params[:xml][:FromUserName] if params[:xml] %>]]></ToUserName>
    <FromUserName><![CDATA[<%= params[:xml][:ToUserName] if params[:xml] %>]]></FromUserName>
    <CreateTime><%= Time.now.to_i %></CreateTime>
    <MsgType><![CDATA[news]]></MsgType>
    <ArticleCount> <%= @objs.length %> </ArticleCount>
    <Articles>
    <% for item in @objs do %>
      <item>
        <Title><![CDATA[ <%= item['title'] %> ]]></Title> 
        <Description><![CDATA[ <%= item['desc'] %> ]]></Description>
        <PicUrl><![CDATA[ <%= item['img'] %> ]]></PicUrl>
        <Url><![CDATA[ <%= item['url'] %> ]]></Url>
      </item>
    <% end %>
    </Articles>
    <FuncFlag>1</FuncFlag>
</xml>
